package-as: SlashRead

license-output: LICENSE

ignore:
  - Libs/*/*.rockspec
  - Libs/*/*.toc
  - Libs/*/changelog.*
  - Libs/*/CHANGELOG.*
  - Libs/*/docs
  - Libs/*/examples
  - Libs/*/README.*
  - Libs/*/tests
  - Libs/*/.pkgmeta
  - README.md

manual-changelog:
  filename: CHANGELOG.md
  markup-type: markdown
